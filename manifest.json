{
  "manifest_version": 3,
  "name": "Open GitHub repository in VS Code",
  "version": "1.0",
  "description": "Open GitHub repository in VS Code",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://github.com/*"]
    }
  ]
}
